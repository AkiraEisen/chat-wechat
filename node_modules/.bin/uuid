#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/heyongquan/work/playground/chat-wechat/node_modules/uuid/dist/bin/node_modules:/Users/heyongquan/work/playground/chat-wechat/node_modules/uuid/dist/node_modules:/Users/heyongquan/work/playground/chat-wechat/node_modules/uuid/node_modules:/Users/heyongquan/work/playground/chat-wechat/node_modules:/Users/heyongquan/work/playground/node_modules:/Users/heyongquan/work/node_modules:/Users/heyongquan/node_modules:/Users/node_modules:/node_modules:/Users/heyongquan/work/playground/chat-wechat/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/heyongquan/work/playground/chat-wechat/node_modules/uuid/dist/bin/node_modules:/Users/heyongquan/work/playground/chat-wechat/node_modules/uuid/dist/node_modules:/Users/heyongquan/work/playground/chat-wechat/node_modules/uuid/node_modules:/Users/heyongquan/work/playground/chat-wechat/node_modules:/Users/heyongquan/work/playground/node_modules:/Users/heyongquan/work/node_modules:/Users/heyongquan/node_modules:/Users/node_modules:/node_modules:/Users/heyongquan/work/playground/chat-wechat/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../uuid/dist/bin/uuid" "$@"
else
  exec node  "$basedir/../uuid/dist/bin/uuid" "$@"
fi
